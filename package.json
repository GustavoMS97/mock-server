{
  "name": "mock-server",
  "version": "0.1.0",
  "description": "mock server to test integrations without it being a pain in the a** when it's validated in the other end",
  "main": "src/app.js",
  "scripts": {
    "start": "node ./src/app.js",
    "start:dev": "nodemon ./src/app.js",
    "start:docker": "docker compose up --build -d",
    "restart:docker": "docker compose down && docker compose up --build -d",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/GustavoMS97/mock-server.git"
  },
  "keywords": [
    "mock",
    "server",
    "integration",
    "node",
    "express",
    "JavaScript"
  ],
  "author": "Gustavo Santos",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/GustavoMS97/mock-server/issues"
  },
  "homepage": "https://github.com/GustavoMS97/mock-server#readme",
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2"
  },
  "devDependencies": {
    "eslint": "8.0.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-n": "15.0.0 || 16.0.0 ",
    "eslint-plugin-prettier": "5.1.3",
